    &lt;meta charset=utf-8 /&gt;
    &lt;title&gt;Get Verses from BHS in Accordance&lt;/title&gt;
    &lt;style type="text/css"&gt;
        body {
            font-family: Arial;
            margin: 5px;
            font-size: 14px;
            background-color: whitesmoke;
        }

        tr {
            height: 30;
        }

        .formatRow {
            height: 20;
        }

        .caption {
            font-weight: bold;
            color:black;
        }

        .buttonDiv {
            margin-top: 0px;
            margin-right: 25px;
            text-align: right;
        }

        .btnDefault,
        .btnCancel {
            border-radius: 8px;
            font-family: Arial;
            color: black;
            font-size: 14px;
            text-decoration: none;
            font-weight: bold;
            margin-left: 15px;
            padding: 5px 20px 5px 20px;
        }

        .btnDefault {
            background: steelblue;
            color: white;
            border: solid royalblue 2px;
        }

        .btnCancel {
            background: white;
            color: black;
            border: solid royalblue 2px;
        }

        .btnDefault:hover,
        .btnCancel:hover {
            background: gold;
            text-decoration: none;
            color: black;
        }
    &lt;/style&gt;
&lt;/head&gt;

&lt;body data-kmwindowid="CustomKMMacroList" data-kmwindow="SCREEN(Front,MidX)-590/2,SCREEN(Front,MidY)-230/2,600,230"&gt;

    &lt;table style="padding: 10px"&gt;
        &lt;colgroup&gt;
            &lt;col style="width: 120px;"&gt;
            &lt;col&gt;
            &lt;col&gt;
        &lt;/colgroup&gt;
        &lt;tbody&gt;
            &lt;tr&gt;
                &lt;td class = "caption"&gt;Reference:&lt;/td&gt;
                &lt;td colspan="2"&gt;&lt;input type="text" style="font-size: 18px" name="BibleRef" id="bbref" autofocus="" /&gt;&lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td class = "caption"&gt;Romove:&lt;/td&gt;
                &lt;td colspan="2"&gt;
                    &lt;input type="radio" name="LocalRemove" value="Nothing" /&gt;&amp;nbsp;Nothing&amp;emsp;
                    &lt;input type="radio" id="RemVowels" name="LocalRemove" value="Cantillations" /&gt;&amp;nbsp;Cantillations&amp;emsp;
