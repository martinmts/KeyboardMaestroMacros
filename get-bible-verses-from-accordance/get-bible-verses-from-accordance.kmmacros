						<string>app = Application('System Events');  
app.includeStandardAdditions = true;  

var ref = app.theClipboard(); 

// the book abbreviations should follow the book names in the exported texts.
// The book names are determined by the settings of Citation in the Preference.
// 1,2,3 John must come before John.
// If you add one item to the refEn list, you must also add a Chinese translation
// in the refCn list.
refEn = ["Gen ", "Exod ", "Lev ", "Num ", "Deut ", "Josh ", "Judg ", "Ruth ", "1 Sam ", "2 Sam ", "1 Kgs ", "2 Kgs ", "1 Chr ", "2 Chr ", "Ezra ", "Neh ", "Esth ", "Job ", "Ps ", "Prov ", "Ecc ", "Song ", "Is ", "Isa ", "Jer ", "Lam ", "Ezek ", "Dan ", "Hos ", "Joel ", "Amos ", "Obad ", "Jonah ", "Mic ", "Nah ", "Hab ", "Zeph ", "Hag ", "Zech ", "Mal ", "Matt ", "Mark ", "Luke ", "1 John ", "2 John ", "3 John ", "John ", "Acts ", "Rom ", "1 Cor ", "2 Cor ", "Gal ", "Eph ", "Phi ", "Col ", "1 Thess ", "2 Thess ", "1 Tim ", "2 Tim ", "Titus ", "Phlm ", "Heb ", "Jas ", "1 Pet ", "2 Pet ", "Jude ", "Rev ", "Pss ", "Philemon "];

refCn = ["创 ", "出 ", "利 ", "民 ", "申 ", "书 ", "士 ", "得 ", "撒上 ", "撒下 ", "王上 ", "王下 ", "代上 ", "代下 ", "拉 ", "尼 ", "斯 ", "伯 ", "诗 ", "箴 ", "传 ", "歌 ", "赛 ", "赛 ", "耶 ", "哀 ", "结 ", "但 ", "何 ", "珥 ", "摩 ", "俄 ", "拿 ", "弥 ", "鸿 ", "哈 ", "番 ", "该 ", "亚 ", "玛 ", "太 ", "可 ", "路 ", "约一 ", "约二 ", "约三 ", "约 ", "徒 ", "罗 ", "林前 ", "林后 ", "加 ", "弗 ", "腓 ", "西 ", "帖前 ", "帖后 ", "提前 ", "提后 ", "多 ", "门 ", "来 ", "雅 ", "彼前 ", "彼后 ", "犹 ", "启 ", "诗 ", "门 "];


for (i = 0; i &lt; refEn.length; i++) {
	if ( ref.includes(refEn[i]) ) {
		ref.replaceAll(refEn[i], refCn[i]);
		break;
	}
}</string>
