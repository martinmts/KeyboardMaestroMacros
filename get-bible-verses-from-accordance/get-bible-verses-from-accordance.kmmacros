						<string>app = Application('System Events');  
app.includeStandardAdditions = true;  

var myStr = app.theClipboard();

var myStrUnicode = toUnicode(myStr);

var cantUnicode = ["\\u0591","\\u0592","\\u0593","\\u0594","\\u0595","\\u0596","\\u0597","\\u0598","\\u0599","\\u059A","\\u059B","\\u059C","\\u059D","\\u059E","\\u059F","\\u05A0","\\u05A1","\\u05A3","\\u05A4","\\u05A5","\\u05A6","\\u05A7","\\u05A8","\\u05A9","\\u05AA","\\u05AB","\\u05AC","\\u05AD","\\u05AE","\\u05AF","\\u05B0","\\u05B1","\\u05B2","\\u05B3","\\u05B4","\\u05B5","\\u05B6","\\u05B7","\\u05B8","\\u05B9","\\u05BB","\\u05BC","\\u05BD","\\u05BE","\\u05BF","\\u05C0","\\u05C4","\\u05F3","\\u05F4"];


cantUnicode.forEach(element =&gt; myStrUnicode = myStrUnicode.replaceAll(element,''));

JSON.parse('"' + myStrUnicode + '"');

function toUnicode(theString) {
  var unicodeString = '';
  for (var i=0; i &lt; theString.length; i++) {
    var theUnicode = theString.charCodeAt(i).toString(16).toUpperCase();
    while (theUnicode.length &lt; 4) {
      theUnicode = '0' + theUnicode;
    }
    theUnicode = "\\u" + theUnicode;
    unicodeString += theUnicode;
  }
  return unicodeString;
}

